        ReDim oFunciones.ParametersX_Global(1)
        oFunciones.ParametersX_Global(0) = New SqlClient.SqlParameter("@ESTADO", "TABASCO")
        oFunciones.ParametersX_Global(1) = New SqlClient.SqlParameter("@MUNICIPIO", CBmunicipio.Text)
